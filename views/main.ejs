    </div>
    <div class="col s4">
    <a href="#"><img id="hero" class="hoverable" src="https://cdn0.vox-cdn.com/thumbor/VR1lcomli21MQ2NBbVTpRBVJ-lI=/0x0:2000x1335/1200x675/filters:focal(840x508:1160x828)/cdn0.vox-cdn.com/uploads/chorus_image/image/54813587/2017_05_10_Rossoblu_003.0.jpg" alt=""></a>
    </div>
  </div>

  <!-- Dropdown Structure -->
  <ul id="maindrop" class="dropdown-content">
    <% venues.forEach(function(venue){ %>
    <li><a href="/venues/<%=venue._id%>/places"><%= venue.name %></a></li>
    <li class="divider"></li>
    <%})%>
  </ul>
  <!-- end of Dropdown Structure -->
  <nav class="main-nav">
    <div class="nav-wrapper red darken-3">
      <ul class="left hide-on-med-and-down">
        <li><a class="dropdown-button" href="#" data-activates="maindrop">SELECT YOUR VENUE<i class="material-icons right">arrow_drop_down</i></a></li>
      </ul>
      <a href="#" data-activates="mobile" class="button-collapse"><i class="material-icons">menu</i></a>
      <a href="/" class="brand-logo center">
        <img width="95" src="http://i.imgur.com/O4w15vh.png" alt="Meal Ticket">
      </a>
      <ul class="right">
        <% if (user && user.admin) { %>
          <li><a href="/venues">Edit Venues</a></li>
        <%}%>
        <% if (user) { %>
          <li><a href="/logout" class="white-text"><i class="material-icons left white-text text-darken-3">trending_flat</i>Logout</a></li>
        <% } else { %>
          <li><a href="/auth/google" class="white-text"><i class="material-icons left white-text text-darken-3">vpn_key</i>Login</a></li>
        <% } %>
      </ul>
      <ul class="side-nav" id="mobile">
        <% venues.forEach(function(venue){ %>
        <li><a href="/venues/<%=venue._id%>/places"><%= venue.name %></a></li>
        <% }) %>
      </ul>
    </div>
    <div class="input-field">
      <input id="search" type="search" required>
      <label class="label-icon" for="search"><i class="material-icons red-text text-darken-3">search</i></label>
      <i class="material-icons">close</i>
    </div>
  </nav>
